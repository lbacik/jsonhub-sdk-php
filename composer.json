{
    "name": "lbacik/jsonhub-sdk",
    "description": "JsonHub SDK (https://jsonhub.cloud)",
    "type": "library",
    "license": "MIT",
    "require": {
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": "^7.8",
        "lbacik/object-collection": "^0.1.0",
        "psr/log": "^3.0",
        "saloonphp/saloon": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "JsonHub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonHub\\SDK\\Tests\\": "tests/SDK/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "squizlabs/php_codesniffer": "^3.9"
    }
}
